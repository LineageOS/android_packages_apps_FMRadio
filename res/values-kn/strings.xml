         [CHAR LIMIT=12] -->
    <string name="toast_listen">ಆಲಿಸು</string>
    <!-- Snackbar message shown after we rename a favorite station [CHAR LIMIT=50] -->
    <string name="toast_station_renamed">ಕೇಂದ್ರ ಮರುನಾಮಕರಣವಾಗಿದೆ</string>
</resources>
