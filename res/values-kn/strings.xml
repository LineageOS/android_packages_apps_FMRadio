    <string name="rename">ಮರುನಾಮ</string>
    <!-- EditText hint for rename station dialog [CHAR LIMIT=50] -->
    <string name="station_rename_hint">ಕೇಂದ್ರದ ಹೆಸರು ನಮೂದಿಸಿ</string>
