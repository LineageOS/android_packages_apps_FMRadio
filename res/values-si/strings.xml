    <string name="toast_cannot_search">තිබෙන ස්ථාන සෙවීම කළ නොහැක.</string>
    <!-- Title of a menu item to change FM sound output to speaker [CHAR LIMIT=20] -->
    <string name="optmenu_speaker">නාදකය</string>
