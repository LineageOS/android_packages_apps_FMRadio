         [CHAR LIMIT=12] -->
    <string name="toast_listen">POSŁUCHAJ</string>
    <!-- Snackbar message shown after we rename a favorite station [CHAR LIMIT=50] -->
    <string name="toast_station_renamed">Nazwa stacji została zmieniona</string>
</resources>
